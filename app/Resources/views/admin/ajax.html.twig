{% for admin in admins %}
    <tr>
        <td class="hidden-print"><input type="checkbox" name="entities[]" value="{{  admin.id}}"></td>
        <td>{{ admin.nom }}</td>
                <td>{{ admin.prenom }}</td>
                <td>{{ admin.username }}</td>
              
                <td>{{ admin.codeEtatCivil }}</td>
                 <td>{{ admin.sexe}}</td>
        <td class="hidden-print">
            <div class="text-center">
                <a class="btn btn-primary btn-xs" data-toggle="tooltip" data-placement="top" title="afficher" href="{{ path('admin_show', { 'id': admin.id }) }}"><span class="glyphicon glyphicon-search"></span></a>
                <a class="btn btn-danger btn-xs" data-toggle="tooltip" data-placement="top" title="modifier" href="{{ path('admin_edit', { 'id': admin.id }) }}"><span class="glyphicon glyphicon-edit"></span></a>
            </div>
        </td>
    </tr>
{% else %}
    <tr class="mypagination">            
        <td colspan="10">
            <div id="msgbox" class="alert alert-danger fade in" role="alert"> 
                <button type="button" class="close" data-dismiss="alert">×</button>
                <strong>info !</strong> aucun résultat ne correspond à vos critères
            </div>
        </td>
    </tr>
{% endfor %}


<script>   
(function($) {
    /* tooltip */
    $('a[data-toggle="tooltip"]').tooltip();
})(jQuery);
</script>
